);

CREATE TABLE Tb_Seria (
    co_ser INT,
    nu_issn char(7),
    nu_ser smallint,
    fk_Tb_Publi_co_publi INT,
    fk_Tb_Publi_fk_Tb_Produ_co_produ INT,
