);

CREATE TABLE Tb_Campa (
    co_campa Int PRIMARY KEY,
    no_campa Varchar(30),
    ds_campa varchar(250),
    dt_inic date,
    vl_durac interval,
    vl_meta int
);

CREATE TABLE Rl_Tb_RCamp_Tb_Produ (
    fk_Tb_RCamp_co_rcamp INT,
    fk_Tb_Produ_co_produ INT,
    qt_produ int,
    PRIMARY KEY (fk_Tb_RCamp_co_rcamp, fk_Tb_Produ_co_produ)
);
 
